CREATE TABLE user(
    id INT PRIMARY KEY AUTO_INCREMENT,
    userName VARCHAR(255),
    email VARCHAR(255),
    passWord VARCHAR(255)

);

CREATE TABLE
    articles(
        id INT PRIMARY KEY AUTO_INCREMENT,
        content VARCHAR(255),
        category_id int,
        author VARCHAR(255),
        FOREIGN KEY(category_id) REFERENCES categories(id) ON UPDATE CASCADE ON DELETE CASCADE
    );